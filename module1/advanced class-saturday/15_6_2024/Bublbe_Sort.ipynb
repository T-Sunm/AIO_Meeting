   "source": [
    "list = [9, 4, 7, 6]\n",
    "length = len(list)\n",
    "for i in range(length):\n",
    "    swap = False\n",
    "    for j in range(length - i - 1):\n",
    "        swap = True\n",
    "        if list[j] > list[j + 1]:\n",
    "            (list[j + 1], list[j]) = (list[j], list[j + 1])\n",
    "    if swap == False:\n",
    "        break\n",
    "\n",
    "print(list)"
   ]
